@bot.message_handler(commands=['description'])
def description(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn1 = types.KeyboardButton('/start')
    markup.add(btn1)
    bot.send_message(message.from_user.id,
                     'üôà–ö–æ–ª–ª–µ–∫—Ü–∏–æ–Ω–Ω—ã–µ –∫–∞—Ä—Ç–æ—á–∫–∏ –∏ –∫–∞—Ä—Ç–æ—á–Ω—ã–µ –∏–≥—Ä—ã –ø–æ—è–≤–∏–ª–∏—Å—å –∑–∞–¥–æ–ª–≥–æ –¥–æ NFT –∏ –æ—Å—Ç–∞—é—Ç—Å—è –ø–æ–ø—É–ª—è—Ä–Ω—ã–º–∏ –µ—â–µ —Å 90-—Ö –≥–æ–¥–æ–≤. '
                     'üìç–°–æ–±–∏—Ä–∞—Ç–µ–ª–∏ —Ç—Ä–∞—Ç—è—Ç –æ–≥—Ä–æ–º–Ω—ã–µ —Å—É–º–º—ã, —á—Ç–æ–±—ã –∑–∞–ø–æ–ª—É—á–∏—Ç—å –≤ —Å–≤–æ—é –∫–æ–ª–ª–µ–∫—Ü–∏—é —Ä–µ–¥–∫–∏–π —ç–∫–∑–µ–º–ø–ª—è—Ä, –±—É–¥—å —Ç–æ –∫–∞—Ä—Ç–æ—á–∫–∞ —Å –ø–æ–∫–µ–º–æ–Ω–æ–º –∏–ª–∏ –¥—Ä—É–≥–∏–º —Ü–µ–Ω–Ω—ã–º –≥–µ—Ä–æ–µ–º. \n'
                     'üìç–ù–µ–∫–æ—Ç–æ—Ä—ã–µ —ç–∫–∑–µ–º–ø–ª—è—Ä—ã –Ω–∞—Å—Ç–æ–ª—å–∫–æ —Ä–µ–¥–∫–∏ –∏ —É–Ω–∏–∫–∞–ª—å–Ω—ã, —á—Ç–æ —Å—Ç–∞–Ω–æ–≤—è—Ç—Å—è –≤—ã–≥–æ–¥–Ω—ã–º–∏ –∞–∫—Ç–∏–≤–∞–º–∏. \n'
                     'üìç–í –Ω–∞—à–µ–º –ø—Ä–æ–µ–∫—Ç–µ –∏–º–µ–µ—Ç—Å—è –±–æ–ª—å—à–æ–µ —Ä–∞–∑–Ω–æ–æ–±—Ä–∞–∑–∏–µ –∫–∞—Ä—Ç–æ—á–µ–∫ (–∫–∞—Ç–µ–≥–æ—Ä–∏–∏: –º–µ–º—ã, NFT –∏ —Ç.–ø.)–∑–∞ –∫–æ—Ç–æ—Ä—ã–µ –≤–∞–º –Ω–µ –ø—Ä–∏–¥–µ—Ç—Å—è –ø–ª–∞—Ç–∏—Ç—å, –Ω—É–∂–Ω–æ –±—É–¥–µ—Ç –ª–∏—à—å –ø–æ–¥–æ–∂–¥–∞—Ç—å –Ω–µ–∫–æ—Ç–æ—Ä–æ–µ –≤—Ä–µ–º—è. \n'
                     'üìç–¢–∞–∫–∂–µ –≤—ã –º–æ–∂–µ—Ç–µ –ø—Ä–æ—Å–º–∞—Ç—Ä–∏–≤–∞—Ç—å —Å–≤–æ—é –∫–æ–ª–ª–µ–∫—Ü–∏—é –∏ —Å—Ä–∞–≤–Ω–∏–≤–∞—Ç—å –µ—ë —Å –¥—Ä—É–≥–∏–º–∏. –ü–æ–µ—Ö–∞–ª–∏!üôà',
                     reply_markup=markup)


@bot.message_handler(content_types=['text'])
def hello(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn1 = types.KeyboardButton('/start')
    btn2 = types.KeyboardButton('/description')
    markup.add(btn1, btn2)
    bot.send_message(message.from_user.id,
                     "üôà–ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É–µ–º, –¥—Ä—É–∂–∏—â–µ! üëÄ–í—ã–±–µ—Ä–∏, —á—Ç–æ —Ö–æ—á–µ—à—å –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å: \n"
                     "üß∑–ò—Å–ø–æ–ª—å–∑—É–π /start , —á—Ç–æ–±—ã –Ω–∞—á–∞—Ç—å. \n"
                     "üß∑–ò—Å–ø–æ–ª—å–∑—É–π /description , —á—Ç–æ–±—ã –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å –∫—Ä–∞—Ç–∫–æ–µ, –Ω–æ –ø–æ–Ω—è—Ç–Ω–æ–µ –æ–ø–∏—Å–∞–Ω–∏–µ —ç—Ç–æ–≥–æ –±–æ—Ç–∞.üôà",
                     reply_markup=markup)


bot.polling(none_stop=True, interval=0)
