    bot.send_dice(message.from_user.id, 'üèÄ')
    bot.send_message(message.from_user.id, 'üôà–ú–∞–π–∫–ª –î–∂–æ—Ä–¥–∞–Ω —Ç–æ–∂–µ –¥–æ–ª–≥–æ —à–µ–ª –∫ —Å–≤–æ–µ–π –∑–∞–≤–µ—Ç–Ω–æ–π —Ü–µ–ª–∏‚õπ‚Äç‚ôÇ!', reply_markup=markup)


